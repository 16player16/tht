import string
from random import *

letters = string.ascii_letters 
digits = string.digits 
symbols = string.punctuation
chars =  symbols + digits + letters

min_u = int(input("min karakter "))
max_u = int(input("max karakter"))
sifre ="".join(choice(chars) for x in range(randint(min_u, max_u)))
print("Şifreniz =",sifre)

